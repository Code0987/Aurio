<Window x:Class="AudioAlign.LibSampleRate.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="156" Width="525" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Button Content="Open File" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Play" Height="23" HorizontalAlignment="Right" Margin="0,12,12,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <Slider Height="23" Margin="97,78,12,0" Name="sliderSampleRate" VerticalAlignment="Top" SmallChange="1" LargeChange="100" Maximum="192000" Minimum="100" Value="44100" AutoToolTipPlacement="TopLeft" />
        <Label Content="Sample Rate:" Height="28" HorizontalAlignment="Left" Margin="12,73,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Label" Height="28" Margin="93,11,93,0" Name="lblFile" VerticalAlignment="Top" />
    </Grid>
</Window>
